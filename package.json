{
  "name": "react-native-app-with-web",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.1",
    "babel-jest": "19.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-react-native": "1.9.1",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "html-webpack-plugin": "^2.28.0",
    "http-serve": "^1.0.1",
    "jest": "19.0.2",
    "jest-expo": "^0.4.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.2",
    "offline-plugin": "^4.7.0",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-native-scripts": "0.0.29",
    "react-test-renderer": "16.0.0-alpha.6",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "web:build:vendor-dev:build": "cross-env NODE_ENV=development webpack --config web/vendor.webpack.config.js",
    "web:build:vendor-dev": "npm-run-all web:clean:vendor-dev web:build:vendor-dev:build web:clean:vendor-dev",
    "web:dev:server": "cross-env NODE_ENV=development webpack-dev-server -d --host 0.0.0.0 --port 3000 --config web/webpack.config.js --inline --hot --colors",
    "web": "npm-run-all web:build:vendor-dev web:dev:server",
    "web:clean:vendor": "rimraf web/vendor",
    "web:clean:vendor-dev": "rimraf web/build",
    "web:clean": "npm-run-all web:clean:*",
    "web:build:vendor:build": "cross-env NODE_ENV=production webpack --config web/vendor.webpack.config.js",
    "web:build:vendor": "npm-run-all web:clean:vendor web:build:vendor:build",
    "web:build:app:build": "cross-env NODE_ENV=production webpack --config web/webpack.config.js",
    "web:build:app": "npm-run-all web:clean:vendor-dev web:build:app:build",
    "web:build": "npm-run-all web:build:vendor web:build:app",
    "web:serve": "http-serve -p 3001 --gzip true ./web/build"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "expo": "^16.0.0",
    "react": "16.0.0-alpha.6",
    "react-native": "^0.43.4",
    "react-native-web": "^0.0.94"
  }
}
